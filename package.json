{
  "name": "sheep-chrome-demo",
  "displayName": "Sheep chrome demo",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "ThinkPad",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.62.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.0",
    "@types/chrome": "0.0.202",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "prettier": "2.7.1",
    "typescript": "4.9.3"
  },
  "manifest": {
    "permissions":["declarativeNetRequest"],

		"web_accessible_resources": [
			{
				"resources": [
					"inject.js"
				],
				"matches": [
					"<all_urls>"
				]
			}
		],
    "host_permissions": [
      "http://wap.rehou.cn/"
    ]
  }
}
